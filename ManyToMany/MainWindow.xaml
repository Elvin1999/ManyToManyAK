<Window x:Class="ManyToMany.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ManyToMany"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <DataGrid Width="500" Name="coursesGrid" SelectionChanged="coursesGrid_SelectionChanged" ></DataGrid>
        <DataGrid Width="500" Name="studentsGrid" SelectionChanged="studentsGrid_SelectionChanged"></DataGrid>
    </StackPanel>
</Window>
